{
  "name": "coworker",
  "description": "Relatable humor: fake Slack messages from a coworker and fortune cookie wisdom.",
  "lines": [
    {
      "left": ["model-name", "directory", "git-branch", "git-status"],
      "right": ["lines-changed", "session-timer"]
    },
    {
      "left": ["context-bar"],
      "right": ["session-cost"]
    },
    {
      "left": ["coworker"],
      "right": ["fortune-cookie"]
    }
  ],
  "separator": " ",
  "plugins": {
    "model-name": {
      "style": "bold cyan"
    },
    "directory": {
      "style": "white",
      "format": "basename"
    },
    "git-branch": {
      "style": "green",
      "icon": false,
      "maxLength": 25
    },
    "git-status": {
      "style": "yellow",
      "format": "short"
    },
    "lines-changed": {
      "style": "dim",
      "hideIfZero": true
    },
    "session-timer": {
      "style": "dim"
    },
    "context-bar": {
      "width": 20,
      "charFilled": "\u2593",
      "charEmpty": "\u2591",
      "warnAt": 60,
      "criticalAt": 80,
      "showPercent": true,
      "styleOk": "green",
      "styleWarn": "bold yellow",
      "styleCritical": "bold red"
    },
    "session-cost": {
      "style": "dim",
      "format": "$"
    },
    "coworker": {
      "style": "italic",
      "botName": "chad"
    },
    "fortune-cookie": {
      "showEmoji": true,
      "style": "dim italic"
    }
  }
}
